{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/rooms(/.*)?",
      "dest": "/backend/api/app.py"
    },
    {
      "src": "/activities(/.*)?",
      "dest": "/backend/api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
