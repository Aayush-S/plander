{
  "version": 2,
  "functions": {
    "backend/api/app.py": {
      "runtime": "@vercel/python",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/rooms(/.*)?",
      "dest": "/backend/api/app.py"
    },
    {
      "src": "/activities(/.*)?",
      "dest": "/backend/api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
